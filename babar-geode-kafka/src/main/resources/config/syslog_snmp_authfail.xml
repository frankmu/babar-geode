<?xml version="1.0" encoding="UTF-8"?>
<rule>
	<name>SNMP-3-AUTHFAIL</name>
	<priority>1</priority>
	<action>+</action>
	<severity>1</severity>
	<node-name>.*</node-name>
	<rule-type>cisco</rule-type>
	<match-pattern>^(\w{3}\s+\d{1,2}\s\d{2}:\d{2}:\d{2})\s([^\s]*)\s[^\s:]*:\s\*(\w{3}\s+\d{1,2}\s\d{2}:\d{2}:\d{2}\.\d{3}):\s([^\s]*)\s%\w*-\d*-\w*:\s(.*)$</match-pattern>
	<variables>
		<variable>syslog-timestamp</variable>
		<variable>ip</variable>
		<variable>incident-timestamp</variable>
		<variable>node-name</variable>
		<variable>msgtxt</variable>
	</variables>
	<fields>
		<field key="alertMsg">$msgtxt at $syslog-timestamp</field>
	</fields>
</rule>